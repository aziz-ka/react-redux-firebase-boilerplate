{
  "name": "react-redux-firebase-boilerplate",
  "version": "0.0.1",
  "description": "Simple React-Redux-Firebase boilerplate",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/aziz-ka/react-redux-firebase-boilerplate.git"
  },
  "author": {
    "name": "Aziz Hasanov",
    "email": "hasanov.aziz@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=5.11"
  },
  "keywords": [
    "react",
    "redux",
    "firebase"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "clean": "del-cli ./build ./coverage",
    "lint": "eslint -c .eslintrc.yml *.js ./client ./server",
    "prebuild": "npm-run-all clean lint test",
    "start": "cross-env NODE_ENV=production nodemon -w 'server/**/*.*' ./server/main.js",
    "server:dev": "cross-env NODE_ENV=dev webpack-dashboard -- webpack-dev-server --progress",
    "dev": "npm run server:dev",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts",
    "test:watch": "npm test -- --watch",
    "test:karma": "cross-env NODE_ENV=test karma start --single-run",
    "test:karma:watch": "cross-env NODE_ENV=test karma start",
    "coverage": "cross-env NODE_ENV=test istanbul cover _mocha -- --opts ./mocha.opts"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
      "babel-plugin-transform-react-constant-elements": "6.9.1",
      "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "classnames": "2.2.5",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "del-cli": "0.2.0",
    "enzyme": "2.6.0",
    "eslint": "3.10.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.6.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "firebase": "3.6.0",
    "firebase-tools": "3.1.0",
    "html-webpack-plugin": "2.24.1",
    "immutable": "3.8.1",
    "istanbul": "1.1.0-alpha.1",
      "jasmine-core": "2.4.1",
    "json-loader": "0.5.4",
      "karma": "1.1.0",
      "karma-chrome-launcher": "1.0.1",
      "karma-jasmine": "1.0.2",
      "karma-mocha": "1.1.1",
      "karma-sourcemap-loader": "0.3.7",
      "karma-webpack": "1.7.0",
    "mocha": "3.1.2",
      "morgan": "1.7.0",
    "node-sass": "3.12.1",
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.1",
    "postcss-loader": "1.1.1",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-saga": "0.12.1",
    "reselect": "2.5.4",
    "sass-loader": "4.0.2",
      "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2",
    "webpack-md5-hash": "0.0.5",
      "winston": "2.3.0"
  }
}
